{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@navigations/*": ["src/navigations/*"],
      "@pages/*": ["src/pages/*"],
      "@themes/*": ["src/themes/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@modules/*": ["src/modules/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@static/*": ["src/static/*"],
      "*": ["src/*"],
    }
  },
}